    <!-- Decorative circles -->
    <circle cx="300" cy="250" r="8" fill="#2196F3"/>
    <circle cx="150" cy="100" r="8" fill="#4CAF50"/>
    <circle cx="450" cy="200" r="8" fill="#9C27B0"/>
    
    <!-- Placeholder text -->
    <text x="300" y="380" 
          font-family="Arial" 
          font-size="24" 
          fill="#666"
          text-anchor="middle">
        Financial Data
    </text>
